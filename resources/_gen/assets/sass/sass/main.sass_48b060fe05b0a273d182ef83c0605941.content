@import "https://fonts.googleapis.com/css2?family=Montserrat&family=Roboto:wght@300;400&display=swap";*,*:before,*:after{box-sizing:border-box}html,body,div,span,object,iframe,figure,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,code,em,img,small,strike,strong,sub,sup,tt,b,u,i,ol,ul,li,fieldset,form,label,table,caption,tbody,tfoot,thead,tr,th,td,main,canvas,embed,footer,header,nav,section,video{margin:0;padding:0;border:0;font-size:100%;font:inherit;vertical-align:baseline;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased;text-size-adjust:none}footer,header,nav,section,main{display:block}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:'';content:none}table{border-collapse:collapse;border-spacing:0}input{-webkit-appearance:none;border-radius:0}.is-bold,strong,b{font-weight:700}em,i{font-style:italic}.is-s,small{font-size:.9rem}.is-l{font-size:1.2rem}.is-xl{font-size:1.4rem}.mb-5{margin-bottom:5px}.mb-10{margin-bottom:10px}.mb-15{margin-bottom:15px}.my-5{margin:5px 0}.my-10{margin:10px 0}.my-15{margin:15px 0}.is-center{text-align:center}body{max-width:700px;width:80%;margin:0 auto;font-size:18px;font-family:roboto,sans-serif;color:#4a4a4a;background-color:#fafafa;margin-top:20px}nav{margin-bottom:20px}nav a{text-decoration:none}ol,ul{list-style:inherit}h1,h2,h3,h4,h5,h6{font-weight:700;font-family:montserrat,sans-serif;margin-bottom:20px}h1{font-size:2rem;line-height:2.8rem}h2{font-size:1.6rem}h3{margin-bottom:10px}p{line-height:2rem}a{color:#145cb8;text-decoration:none}a:hover{color:#334a67;text-decoration:underline}hr{margin:20px 0}code{background-color:#dfdfdf;font-size:1rem;padding:2px;font-family:monospace}pre code{border-radius:10px;margin:10px 0;font-family:monospace;line-height:1.2rem}.is-flex{display:flex;flex-wrap:wrap;justify-content:space-between;align-items:center}.is-flex>div{width:30%;margin-bottom:30px}.is-flex>div p{line-height:1.4rem}.article h2{margin-top:40px}.article h2::before{content:'# '}.article h3{margin-top:25px}.article p:not(:first-child),.article img:not(:first-child),.article blockquote:not(:first-child){margin-top:10px}.article li{list-style-position:inside;margin:10px 0;line-height:1.5rem}.article blockquote{padding-left:10px;border-left:2px solid #c9c9c9;font-style:italic}.labels{display:inline-block;background-color:rgba(224,233,241,.5);padding:3px 5px}.labels a{color:#5a5f81}.labels:hover{background-color:#e3ffaa}.labels:hover a{text-decoration:none}button:hover{cursor:pointer}.series li{margin-bottom:12px}footer{margin:50px 0}@media only screen and (max-width:760px){body{width:90%}h2{line-height:2rem}h3{line-height:1.8rem}.is-flex{display:block}.is-flex div{width:100%}}